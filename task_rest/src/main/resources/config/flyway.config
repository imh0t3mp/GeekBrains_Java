flyway.url=${JDBC_DATABASE_URL:jdbc:postgresql://localhost:5432/task_app}
flyway.username=${JDBC_DATABASE_USERNAME:postgres}
flyway.password=${JDBC_DATABASE_PASSWORD:postgres}
flyway.schemas=${JDBC_SCHEMA:public}
flyway.enabled=true
flyway.locations=classpath:db/migration
flyway.baseline-on-migrate=true